{{#each this}}
  <div class="row">
    
    <div class="col-xs-8">
      <ul style="list-style-type:none; ">
          <li id="Name"><h4>{{Name}}</h4></li>
          <li id="Quote" style="display:inline;">{{Quote}} <span>   </span> </li>
          <!-- <li class="NumVotes" style="display:inline; float:right;">{{Votes}}</li> -->
      </ul>
    </div>


    <div class="row">
        <div class="col-xs-1">
          <p class="NumVotes">{{Votes}}</p>
        </div>    
        <div class="col-xs-3">
          <div class="btn-group-sm VoteIcons" >              
            <button type="button" id={{id}} class="btn btn-default VoteButton Up"> <span class="glyphicon glyphicon-thumbs-up"></span></button>
            <button type="button" id={{id}} class="btn btn-default VoteButton Down"> <span class="glyphicon glyphicon-thumbs-down"></span></button>
          </div>
        </div>
    </div>
      
  </div>

  <br>
  <br>
  <br>
{{/each}}